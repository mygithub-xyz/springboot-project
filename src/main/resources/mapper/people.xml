<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 直接使用代理的方式 -->
<mapper namespace="com.offcn.dao.PeopleDao">
    <select id="findByPnameLike" parameterType="string" resultType="com.offcn.pojo.People">
        select *from people where pname like #{name}
    </select>
</mapper>
